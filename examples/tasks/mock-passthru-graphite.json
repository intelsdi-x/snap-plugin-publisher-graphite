{
    "version": 1,
    "schedule": {
        "type": "simple",
        "interval": "1s"
    },
    "workflow": {
        "collect": {
            "metrics": {
                "/intel/mock/*": {}
            },
            "config": {
                "/intel/mock": {
                    "user": "root",
                    "password": "secret"
                }
            },
            "process": [
                {
                    "plugin_name": "passthru-grpc",
                    "publish": [
                        {
                            "plugin_name": "graphite",
                            "config": {
                                "server": "graphite",
                                "log-level": "debug",
                                "prefix": "myprefix",
                                "port": 2003
                            }
                        }
                    ]
                }
            ]
        }
    }
}